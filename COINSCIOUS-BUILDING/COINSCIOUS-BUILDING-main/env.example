# COINSCIOUS Security Token Platform Environment Variables
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/coinscious?schema=public

# API Configuration
JWT_SECRET=your_jwt_secret_here_minimum_32_characters
KYC_HMAC_SECRET=your_kyc_hmac_secret_here_minimum_32_characters

# Blockchain Configuration
ETH_RPC_URL=https://sepolia.base.org
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Contract Addresses (will be filled after deployment)
COMPLIANCE_REGISTRY_ADDRESS=
SECURITY_TOKEN_FACTORY_ADDRESS=
PAYOUT_DISTRIBUTOR_FACTORY_ADDRESS=

# External API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key_here
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Compliance Configuration
ORACLE_ADDRESS=your_oracle_wallet_address_here
ADMIN_ADDRESS=your_admin_wallet_address_here

# Pilot Configuration
PILOT_MODE=true
DEBUG_MODE=true
